{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "angular-mentions-2": [
        "dist/angular-mentions-2"
      ],
      "angular-mentions-2/*": [
        "dist/angular-mentions-2/*"
      ],
      "mention": [
        "dist/mention"
      ],
      "mention/*": [
        "dist/mention/*"
      ]
    }
  }
}